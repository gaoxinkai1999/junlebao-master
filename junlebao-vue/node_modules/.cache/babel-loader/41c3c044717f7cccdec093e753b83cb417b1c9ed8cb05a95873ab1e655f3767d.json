{"ast":null,"code":"export default {\n  name: \"AddOrder\",\n  data() {\n    return {\n      order: {\n        shop: '',\n        date: ''\n      },\n      rules: {\n        shop: {\n          required: true,\n          message: '请输入活动名称',\n          trigger: 'blur'\n        },\n        date: {\n          required: true,\n          message: '请输入活动名称',\n          trigger: 'blur'\n        }\n      },\n      props: {\n        label: 'name',\n        value: 'name',\n        children: 'shops',\n        emitPath: false\n      },\n      a: []\n    };\n  },\n  methods: {\n    //发送添加请求\n    addOrder() {\n      this.$http.post(\"/addOrder\", this.order).then(res => {\n        console.log(res);\n      });\n    },\n    //规则校验\n    submitForm(formName) {\n      this.$refs[formName].validate(valid => {\n        if (valid) {\n          this.open();\n        } else {\n          console.log('error submit!!');\n          return false;\n        }\n      });\n    },\n    //确认是否添加\n    open() {\n      this.$confirm('确认操作?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'info'\n      }).then(() => {\n        this.addOrder();\n        this.$message({\n          type: 'success',\n          message: '操作成功!'\n        });\n      }).catch(() => {\n        this.$message({\n          type: 'info',\n          message: '已取消操作'\n        });\n      });\n    }\n  },\n  created() {\n    console.log('加载级联选择数据');\n    this.$http.post('/getLocationsCascader').then(res => {\n      this.a = res.data;\n    });\n  }\n};","map":{"version":3,"names":["name","data","order","shop","date","rules","required","message","trigger","props","label","value","children","emitPath","a","methods","addOrder","$http","post","then","res","console","log","submitForm","formName","$refs","validate","valid","open","$confirm","confirmButtonText","cancelButtonText","type","$message","catch","created"],"sources":["src/components/AddOrder.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <el-form :model=\"order\" :rules=\"rules\" ref=\"order\" class=\"demo-ruleForm\" label-position=\"right\" >\r\n      <h3>商家</h3>\r\n      <el-form-item  prop=\"shop\" >\r\n        <el-cascader\r\n            v-model=\"order.shop\"\r\n            :options=\"a\"\r\n            :props=\"props\" style=\"margin-top:30px;\" placeholder=\"选择商家\">\r\n        </el-cascader>\r\n      </el-form-item>\r\n      <h3>日期</h3>\r\n      <el-form-item  prop=\"date\" >\r\n        <el-date-picker\r\n            v-model=\"order.date\"\r\n            type=\"date\"\r\n            placeholder=\"选择日期\" value-format=\"yyyy-MM-dd\" :editable=\"false\">\r\n        </el-date-picker>\r\n      </el-form-item>\r\n      <el-form-item>\r\n        <el-button type=\"primary\" round @click=\"submitForm('order')\" style=\"margin-top:100px;\">添加</el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"AddOrder\",\r\n  data() {\r\n    return {\r\n      order: {\r\n        shop: '',\r\n        date: ''\r\n      },\r\n      rules: {\r\n        shop:{ required: true, message: '请输入活动名称', trigger: 'blur' },\r\n        date:{ required: true, message: '请输入活动名称', trigger: 'blur' },\r\n      },\r\n      props: {\r\n        label: 'name',\r\n        value: 'name',\r\n        children: 'shops',\r\n        emitPath: false\r\n      },\r\n      a: []\r\n\r\n    }\r\n  },\r\n  methods: {\r\n    //发送添加请求\r\n    addOrder() {\r\n      this.$http.post(\"/addOrder\", this.order).then(res => {\r\n        console.log(res)\r\n      })\r\n    },\r\n    //规则校验\r\n    submitForm(formName) {\r\n      this.$refs[formName].validate((valid) => {\r\n        if (valid) {\r\n         this.open()\r\n        } else {\r\n          console.log('error submit!!');\r\n          return false;\r\n        }\r\n      });\r\n    },\r\n    //确认是否添加\r\n    open() {\r\n      this.$confirm('确认操作?', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'info'\r\n      }).then(() => {\r\n\r\n        this.addOrder()\r\n\r\n        this.$message({\r\n          type: 'success',\r\n          message: '操作成功!'\r\n        });\r\n      }).catch(() => {\r\n        this.$message({\r\n          type: 'info',\r\n          message: '已取消操作'\r\n        });\r\n      });\r\n    }\r\n\r\n  },\r\n  created() {\r\n    console.log('加载级联选择数据')\r\n    this.$http.post('/getLocationsCascader').then(res => {\r\n      this.a = res.data\r\n    })\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n*{\r\n  text-align: center;\r\n}\r\n\r\n</style>\r\n"],"mappings":"AA4BA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,KAAA;QACAC,IAAA;QACAC,IAAA;MACA;MACAC,KAAA;QACAF,IAAA;UAAAG,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAJ,IAAA;UAAAE,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;MACA;MACAC,KAAA;QACAC,KAAA;QACAC,KAAA;QACAC,QAAA;QACAC,QAAA;MACA;MACAC,CAAA;IAEA;EACA;EACAC,OAAA;IACA;IACAC,SAAA;MACA,KAAAC,KAAA,CAAAC,IAAA,mBAAAhB,KAAA,EAAAiB,IAAA,CAAAC,GAAA;QACAC,OAAA,CAAAC,GAAA,CAAAF,GAAA;MACA;IACA;IACA;IACAG,WAAAC,QAAA;MACA,KAAAC,KAAA,CAAAD,QAAA,EAAAE,QAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UACA,KAAAC,IAAA;QACA;UACAP,OAAA,CAAAC,GAAA;UACA;QACA;MACA;IACA;IACA;IACAM,KAAA;MACA,KAAAC,QAAA;QACAC,iBAAA;QACAC,gBAAA;QACAC,IAAA;MACA,GAAAb,IAAA;QAEA,KAAAH,QAAA;QAEA,KAAAiB,QAAA;UACAD,IAAA;UACAzB,OAAA;QACA;MACA,GAAA2B,KAAA;QACA,KAAAD,QAAA;UACAD,IAAA;UACAzB,OAAA;QACA;MACA;IACA;EAEA;EACA4B,QAAA;IACAd,OAAA,CAAAC,GAAA;IACA,KAAAL,KAAA,CAAAC,IAAA,0BAAAC,IAAA,CAAAC,GAAA;MACA,KAAAN,CAAA,GAAAM,GAAA,CAAAnB,IAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}